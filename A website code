	<div class="container">
	  <div class="Hehe" id="Hehe"></div>
	  <p class="counter" id="Hehe-count">0</p>
	  <button class="click-btn" id="click-btn">Click the Hehe!</button>
	</div>  
	  <script>
    const HeheCountEl = document.getElementById('Hehe-count');
    const clickBtn = document.getElementById('click-btn');
    let Hehe = 0;

    clickBtn.addEventListener('click', () => {
      Hehe++;
      HeheCountEl.textContent = Hehe;
    });
  </script>
	  

